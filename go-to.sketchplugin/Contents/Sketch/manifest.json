{
  "name": "Go To",
  "description" : "A plugin to go to the next or previous artboard.",
  "author" : "Triggvy Gunderson",
  "homepage": "http://github.com/piffpaffpuff",
  "version": "1.0",
  "identifier": "com.github.piffpaffpuff.go-to",
  "commands": [
    {
      "name": "Next Artboard",
      "shortcut": "cmd shift .",
      "script": "script.cocoascript",
      "identifier": "nextArtboard",
      "handler": "onNextArtboard"
    },
    {
      "name": "Previous Artboard",
      "shortcut": "cmd shift ,",
      "script": "script.cocoascript",
      "identifier": "previousArtboard",
      "handler": "onPreviousArtboard"
    }
  ],
  "menu": {
    "title": "Go To",
    "items": [
      "nextArtboard",
      "previousArtboard",
    ]
  }
}